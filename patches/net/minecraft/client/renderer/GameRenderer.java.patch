+  		tes.getBuffer().pos(pos.x, pos.y, pos.z).color(r, g, b, a).endVertex();
+
+  		for (int i=0;i<edges + 1;i++)
+  		{
+  			float startAngle;
+  			startAngle = ( (float) (i) / (float) edges ) * (float) Math.PI * 2.0f;
+
+  			if (side == 0 || side == 1) { //y
+  				float x = (float) pos.x + (float) Math.cos(startAngle) * radius;
+  				float y = (float) pos.y;
